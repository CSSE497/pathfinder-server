# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
GET           /socket               io.pathfinder.controllers.Application.socket

GET           /vehicle              io.pathfinder.controllers.VehicleController.getAll
GET           /vehicle/:id          io.pathfinder.controllers.VehicleController.get(id: Long)
POST          /vehicle              io.pathfinder.controllers.VehicleController.post
PUT           /vehicle/:id          io.pathfinder.controllers.VehicleController.put(id: Long)
DELETE        /vehicle/:id          io.pathfinder.controllers.VehicleController.delete(id: Long)

GET           /commodity            io.pathfinder.controllers.CommodityController.getCommodities()
GET           /commodity/:id        io.pathfinder.controllers.CommodityController.getCommodity(id: Long)
POST          /commodity            io.pathfinder.controllers.CommodityController.createCommodity()
PUT           /commodity/:id        io.pathfinder.controllers.CommodityController.editCommodity(id: Long)
DELETE        /commodity/:id        io.pathfinder.controllers.CommodityController.deleteCommodity(id: Long)

GET           /cluster              io.pathfinder.controllers.ClusterController.getClusters()
GET           /cluster/:id          io.pathfinder.controllers.ClusterController.getCluster(id: Long)
POST          /cluster              io.pathfinder.controllers.ClusterController.createCluster()
DELETE        /cluster/:id          io.pathfinder.controllers.ClusterController.deleteCluster(id: Long)

GET           /application/:id      io.pathfinder.controllers.PathFinderApplicationController.get(id: String)
