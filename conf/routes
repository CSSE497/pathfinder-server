# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
GET           /socket               io.pathfinder.controllers.Application.socket

GET           /vehicle              io.pathfinder.controllers.VehicleController.getAll
GET           /vehicle/:id          io.pathfinder.controllers.VehicleController.get(id: Long)
POST          /vehicle              io.pathfinder.controllers.VehicleController.post
PUT           /vehicle/:id          io.pathfinder.controllers.VehicleController.put(id: Long)
DELETE        /vehicle/:id          io.pathfinder.controllers.VehicleController.delete(id: Long)

GET           /commodity            io.pathfinder.controllers.CommodityController.getAll
GET           /commodity/:id        io.pathfinder.controllers.CommodityController.get(id: Long)
POST          /commodity            io.pathfinder.controllers.CommodityController.post
PUT           /commodity/:id        io.pathfinder.controllers.CommodityController.put(id: Long)
DELETE        /commodity/:id        io.pathfinder.controllers.CommodityController.delete(id: Long)

GET           /cluster              io.pathfinder.controllers.ClusterController.getAll
GET           /cluster/:id          io.pathfinder.controllers.ClusterController.get(id: String)
POST          /cluster              io.pathfinder.controllers.ClusterController.post
DELETE        /cluster/:id          io.pathfinder.controllers.ClusterController.delete(id: String)
